{
    "metadata": {
        "schema_version": "0.1.6",
        "content_version": "1.0.0",
        "mkdocs_version": "1.6.1",
        "material_theme_version": "9.6.17",
        "module_name": "mkdocs",
        "description": "Mkdocs documentation server",
        "enabled": true
    },
    "config": {
        "directories": {
            "install_dir": {
                "path": "/opt/mkdocs",
                "mode": "755"
            },
            "config_dir": {
                "path": "/etc/mkdocs",
                "mode": "755"
            },
            "data_dir": {
                "path": "/var/lib/mkdocs",
                "mode": "755"
            },
            "docs_dir": {
                "path": "/opt/docs/docs",
                "mode": "755"
            }
        },
        "installation": {
            "mkdocs_repo": "https://api.github.com/repos/mkdocs/mkdocs/releases/latest",
            "material_theme_repo": "https://api.github.com/repos/squidfunk/mkdocs-material/releases/latest"
        },
        "documentation": {
            "repository_url": "https://github.com/homeserversltd/documentation.git",
            "local_path": "/opt/docs/docs",
            "version_file": "/opt/docs/VERSION"
        },
        "permissions": {
            "owner": "owner",
            "group": "owner"
        }
    }
} 