{
    "metadata": {
        "schema_version": "0.1.11",
        "module_name": "atuin",
        "description": "Atuin shell history management tool",
        "enabled": true
    },
    "config": {
        "directories": {
            "atuin_root": {
                "path": "/home/owner/.atuin",
                "mode": "755",
                "user_relative": true
            },
            "atuin_bin": {
                "path": "/home/owner/.atuin/bin/atuin",
                "mode": "755",
                "user_relative": true
            },
            "atuin_env": {
                "path": "/home/owner/.atuin/bin/env",
                "mode": "644",
                "user_relative": true
            },
            "config_dir": {
                "path": "/home/owner/.config/atuin",
                "mode": "755",
                "user_relative": true
            },
            "data_dir": {
                "path": "/home/owner/.local/share/atuin",
                "mode": "755",
                "user_relative": true
            }
        },
        "installation": {
            "install_script_url": "https://setup.atuin.sh",
            "github_api_url": "https://api.github.com/repos/atuinsh/atuin/releases/latest"
        },
        "permissions": {
            "owner": "owner",
            "group": "owner"
        }
    }
}